{
  "name": "fine-import",
  "version": "0.0.10",
  "description": "A React component for importing files that's just about fine.",
  "keywords": [
    "react",
    "file-import",
    "file-upload"
  ],
  "author": "@WhatFor",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "pnpm@7.26.0",
  "files": [
    "lib",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WhatFor/fine-import.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://WhatFor.github.io/fine-import",
  "main": "./lib/fine-import.umd.js",
  "module": "./lib/fine-import.es.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/fine-import.es.js",
      "require": "./lib/fine-import.umd.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build --outDir lib",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "npm-publish": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.2.3",
    "@storybook/addon-interactions": "7.2.3",
    "@storybook/addon-links": "7.2.3",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "7.2.3",
    "@storybook/react": "7.2.3",
    "@storybook/react-vite": "7.2.3",
    "@storybook/testing-library": "0.2.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.0",
    "rollup-plugin-sass": "^1.12.20",
    "rollup-plugin-typescript2": "^0.35.0",
    "storybook": "^7.2.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "papaparse": "^5.4.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  }
}
