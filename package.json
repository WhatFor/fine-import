{
  "name": "fine-import",
  "version": "0.0.5",
  "description": "A React component for importing files that's just about fine.",
  "keywords": [
    "react",
    "file-import",
    "file-upload"
  ],
  "author": "@WhatFor",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "pnpm@7.26.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/WhatFor/fine-import.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://WhatFor.github.io/fine-import",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "npm-publish": "pnpm publish --no-git-checks --access public ./lib"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.2.3",
    "@storybook/addon-interactions": "7.2.3",
    "@storybook/addon-links": "7.2.3",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "7.2.3",
    "@storybook/react": "7.2.3",
    "@storybook/react-vite": "7.2.3",
    "@storybook/testing-library": "0.2.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
